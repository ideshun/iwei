/* pages/classify.wxss */
.cla-outer{
  margin: 40rpx;overflow: hidden;
}
.cla-inner{
  width: 25%;float: left;text-align: center;display: block;margin-bottom: 15px;
}
.cla-inner image{
  width: 20px;height: 20px;display: inline-block;
  padding: 15px;background: #f5f5f5;border-radius: 50%;
}
.cla-inner text{
  font-size: 10px;display: block;
}


